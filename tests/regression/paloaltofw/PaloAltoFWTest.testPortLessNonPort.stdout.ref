<?xml version="1.0" ?>
<config version="8.1.0" urldb="paloaltonetworks">
  <devices>
    <entry name="localhost.localdomain">
      <vsys>
        <entry name="vsys1">
          <application/>
          <application-group/>
          <!-- Services -->
          <service>
            <entry name="any-udp">
              <protocol>
                <udp>
                  <port>0-65535</port>
                </udp>
              </protocol>
            </entry>
          </service>
          <!-- Rules -->
          <rulebase>
            <security>
              <rules>
                <entry name="rule-1-1">
                  <to>
                    <member>untrust</member>
                  </to>
                  <from>
                    <member>trust</member>
                  </from>
                  <source>
                    <member>any-ipv4</member>
                  </source>
                  <destination>
                    <member>any-ipv4</member>
                  </destination>
                  <service>
                    <member>any-udp</member>
                  </service>
                  <action>allow</action>
                  <rule-type>interzone</rule-type>
                  <application>
                    <member>any</member>
                  </application>
                </entry>
                <entry name="rule-1-2">
                  <to>
                    <member>untrust</member>
                  </to>
                  <from>
                    <member>trust</member>
                  </from>
                  <source>
                    <member>any-ipv4</member>
                  </source>
                  <destination>
                    <member>any-ipv4</member>
                  </destination>
                  <service>
                    <member>application-default</member>
                  </service>
                  <action>allow</action>
                  <rule-type>interzone</rule-type>
                  <application>
                    <member>icmp</member>
                  </application>
                </entry>
              </rules>
            </security>
          </rulebase>
          <!-- Address Groups -->
          <address-group/>
          <!-- Addresses -->
          <address>
            <entry name="any-ipv4">
              <description>Object to match all IPv4 addresses; negate to match all IPv6 addresses.</description>
              <ip-range>0.0.0.0-255.255.255.255</ip-range>
            </entry>
          </address>
          <tag/>
        </entry>
      </vsys>
    </entry>
  </devices>
</config>

