<?xml version="1.0" ?>
<config version="8.1.0" urldb="paloaltonetworks">
  <devices>
    <entry name="localhost.localdomain">
      <vsys>
        <entry name="vsys1">
          <application/>
          <application-group/>
          <!-- Services -->
          <service/>
          <!-- Rules -->
          <rulebase>
            <security>
              <rules>
                <entry name="policy-1">
                  <to>
                    <member>untrust</member>
                  </to>
                  <from>
                    <member>trust</member>
                  </from>
                  <source>
                    <member>any-ipv4</member>
                  </source>
                  <destination>
                    <member>any-ipv4</member>
                  </destination>
                  <service>
                    <member>application-default</member>
                  </service>
                  <action>allow</action>
                  <rule-type>interzone</rule-type>
                  <application>
                    <member>ssh</member>
                  </application>
                  <tag>
                    <member>trust_untrust_policy-comment-1</member>
                  </tag>
                </entry>
                <entry name="policy-2">
                  <to>
                    <member>untrust</member>
                  </to>
                  <from>
                    <member>trust</member>
                  </from>
                  <source>
                    <member>any-ipv4</member>
                  </source>
                  <destination>
                    <member>any-ipv4</member>
                  </destination>
                  <service>
                    <member>application-default</member>
                  </service>
                  <action>allow</action>
                  <rule-type>interzone</rule-type>
                  <application>
                    <member>web-browsing</member>
                  </application>
                  <tag>
                    <member>trust_untrust_policy-comment-1</member>
                  </tag>
                </entry>
                <entry name="policy-3">
                  <to>
                    <member>dmz</member>
                  </to>
                  <from>
                    <member>trust</member>
                  </from>
                  <source>
                    <member>any-ipv4</member>
                  </source>
                  <destination>
                    <member>any-ipv4</member>
                  </destination>
                  <service>
                    <member>application-default</member>
                  </service>
                  <action>allow</action>
                  <rule-type>interzone</rule-type>
                  <application>
                    <member>web-browsing</member>
                  </application>
                  <tag>
                    <member>trust_dmz_policy-comment-2</member>
                  </tag>
                </entry>
                <entry name="policy-4">
                  <to>
                    <member>dmz-2</member>
                  </to>
                  <from>
                    <member>trust</member>
                  </from>
                  <source>
                    <member>any-ipv4</member>
                  </source>
                  <destination>
                    <member>any-ipv4</member>
                  </destination>
                  <service>
                    <member>application-default</member>
                  </service>
                  <action>allow</action>
                  <rule-type>interzone</rule-type>
                  <application>
                    <member>web-browsing</member>
                  </application>
                </entry>
              </rules>
            </security>
          </rulebase>
          <!-- Address Groups -->
          <address-group/>
          <!-- Addresses -->
          <address>
            <entry name="any-ipv4">
              <description>Object to match all IPv4 addresses; negate to match all IPv6 addresses.</description>
              <ip-range>0.0.0.0-255.255.255.255</ip-range>
            </entry>
          </address>
          <tag>
            <entry name="trust_untrust_policy-comment-1">
              <comments>comment 1 comment 2</comments>
            </entry>
            <entry name="trust_dmz_policy-comment-2">
              <comments>comment 3</comments>
            </entry>
          </tag>
        </entry>
      </vsys>
    </entry>
  </devices>
</config>

