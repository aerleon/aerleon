<?xml version="1.0" ?>
<config version="8.1.0" urldb="paloaltonetworks">
  <devices>
    <entry name="localhost.localdomain">
      <vsys>
        <entry name="vsys1">
          <application/>
          <application-group/>
          <!-- Services -->
          <service>
            <entry name="any-tcp">
              <protocol>
                <tcp>
                  <port>0-65535</port>
                </tcp>
              </protocol>
            </entry>
          </service>
          <!-- Rules -->
          <rulebase>
            <security>
              <rules>
                <entry name="rule-1">
                  <to>
                    <member>untrust</member>
                  </to>
                  <from>
                    <member>trust</member>
                  </from>
                  <source>
                    <member>10.1.0.0/24</member>
                  </source>
                  <destination>
                    <member>10.2.0.0/24</member>
                    <member>10.3.1.0/24</member>
                    <member>10.3.2.0/24</member>
                  </destination>
                  <service>
                    <member>any-tcp</member>
                  </service>
                  <action>allow</action>
                  <rule-type>interzone</rule-type>
                  <application>
                    <member>any</member>
                  </application>
                </entry>
              </rules>
            </security>
          </rulebase>
          <!-- Address Groups -->
          <address-group/>
          <!-- Addresses -->
          <address/>
          <tag/>
        </entry>
      </vsys>
    </entry>
  </devices>
</config>

