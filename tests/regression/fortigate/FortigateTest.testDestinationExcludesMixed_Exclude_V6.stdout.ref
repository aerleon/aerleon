config firewall address
    edit MIXED_P_0
        set type ipmask
        set subnet 1.1.1.1 255.255.255.255
    next
end
config firewall addrgrp
    edit MIXED_P
        set member "MIXED_P_0"
    next
end
config firewall address6
    edit MIXED_P_0
        set type ipprefix
        set ip6 2001:db8::1/128
    next
    edit V6_X_0
        set type ipprefix
        set ip6 2001:db8::1/128
    next
    edit V6_X_1
        set type ipprefix
        set ip6 2001:db8::2/128
    next
end
config firewall addrgrp6
    edit MIXED_P
        set member "MIXED_P_0"
    next
    edit V6_X
        set member "V6_X_0" "V6_X_1"
    next
    edit good-term-1-destination
        set member "MIXED_P"
        set exclude enable
        set exclude-member "V6_X"
    next
end
config firewall policy
    edit 1
        set name "good-term-1"
        set srcintf "port1"
        set dstintf "port2"
        set action accept
        set srcaddr "all"
        set dstaddr "MIXED_P"
        set srcaddr6 "all"
        set dstaddr6 "good-term-1-destination"
        set schedule "always"
        set service "ALL"
        set logtraffic disable
    next
end
