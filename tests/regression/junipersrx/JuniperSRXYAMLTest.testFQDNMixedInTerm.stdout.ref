security {
    replace: address-book {
        global {
                address GOOGLE_DNS_0 {
                    dns-name dns.google.com;
                }
                address-set GOOGLE_DNS {
                    address GOOGLE_DNS_0;
                }
        }
    }
    /*
    $Id:$
    $Date:$
    $Revision:$
    */
    replace: policies {
        /*
        This is a test acl with a comment
        */
        from-zone trust to-zone untrust {
            policy fqdn-term {
                match {
                    source-address any;
                    destination-address GOOGLE_DNS
                    application any;
                }
                then {
                    permit;
                }
            }
        }
    }
}
delete: applications;
